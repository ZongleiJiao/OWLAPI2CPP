    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></owl:versionInfo>
    <enables>
      <Step rdf:ID="HeatCream">
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        ></owl:versionInfo>
        <enables>
          <Step rdf:ID="GraduallyMix">
            <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            ></owl:versionInfo>
            <enables>
              <Step rdf:ID="CookCustard">
                <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                ></owl:versionInfo>
                <enables>
                  <Step rdf:ID="Chill">
                    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    ></owl:versionInfo>
                    <enables>
                      <Step rdf:ID="AddMilk">
                        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                        ></owl:versionInfo>
                        <enables>
                          <Step rdf:ID="TurnInFreezer">
                            <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                            ></owl:versionInfo>
                            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                            >Turn in freezer</rdfs:label>
                          </Step>
                        </enables>
                        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                        >Add milk</rdfs:label>
                      </Step>
                    </enables>
                    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                    >Chill</rdfs:label>
                  </Step>
                </enables>
                <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                >Cook custard</rdfs:label>
              </Step>
            </enables>
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Gradually mix</rdfs:label>
          </Step>
        </enables>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Heat cream</rdfs:label>
      </Step>
    </enables>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Slice bean</rdfs:label>
  </Step>
  <Step rdf:ID="SeparateEggs">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></owl:versionInfo>
    <enables>
      <Step rdf:ID="AddSugar">
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        ></owl:versionInfo>
        <enables>
          <Step rdf:ID="BeatEggs">
            <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            ></owl:versionInfo>
            <enables rdf:resource="#GraduallyMix"/>
            <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
            >Beat eggs</rdfs:label>
          </Step>
        </enables>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
        >Add sugar</rdfs:label>
      </Step>
    </enables>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Separate eggs</rdfs:label>
  </Step>
