]>


<rdf:RDF xmlns="http://www.wyner.info/research/case-ontology.owl#"
     xml:base="http://www.wyner.info/research/case-ontology.owl"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:case-ontology="http://www.wyner.info/research/case-ontology.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:legal-case-ontology="http://www.best-project.nl/owl/legal-case-ontology.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="">
        <rdfs:comment
            >Copyright (c) 2009 Adam Wyner and Rinke Hoekstra</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#benefits -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;benefits">
        <rdfs:domain rdf:resource="&legal-case-ontology;Decided_Case"/>
        <rdfs:range rdf:resource="&legal-case-ontology;Party"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&legal-case-ontology;decision"/>
            <rdf:Description rdf:about="&legal-case-ontology;favours"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#cited_by -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;cited_by">
        <owl:inverseOf rdf:resource="&legal-case-ontology;cites"/>
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;precedes"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#cites -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;cites">
        <rdfs:comment rdf:datatype="&xsd;string"
            >Replaced someValuesFrom restriction with domain and range: not all cases are cited</rdfs:comment>
        <rdfs:domain rdf:resource="&legal-case-ontology;Case"/>
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;succeeds"/>
        <rdfs:range rdf:resource="#Legal_Document"/>
        <rdfs:subPropertyOf rdf:resource="#element"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&legal-case-ontology;decision"/>
            <rdf:Description rdf:about="&legal-case-ontology;rationale"/>
            <rdf:Description rdf:about="#legal_document"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#criticizes -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;criticizes">
        <rdfs:range rdf:resource="&legal-case-ontology;Decided_Case"/>
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;cites"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#decision -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;decision">
        <rdfs:range rdf:resource="&legal-case-ontology;Decision"/>
        <owl:inverseOf rdf:resource="#decision_of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#defendant -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;defendant">
        <rdfs:range rdf:resource="&legal-case-ontology;Defendant"/>
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;party"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#defendant_solicitor -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;defendant_solicitor">
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;party_representation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#distinguishes -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;distinguishes">
        <rdfs:range rdf:resource="&legal-case-ontology;Decided_Case"/>
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;cites"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#expert_witness -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;expert_witness">
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;witness"/>
        <rdfs:range rdf:resource="#Expert_Witness"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&legal-case-ontology;decision"/>
            <rdf:Description rdf:about="&legal-case-ontology;rationale"/>
            <rdf:Description rdf:about="#expert_testimony"/>
            <rdf:Description rdf:about="#given_by"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#factor -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;factor">
        <rdfs:subPropertyOf rdf:resource="#argues"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#favours -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;favours"/>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#hearing -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;hearing">
        <rdfs:domain rdf:resource="&legal-case-ontology;Case"/>
        <rdfs:range rdf:resource="&legal-case-ontology;Hearing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#issue -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;issue">
        <rdfs:subPropertyOf rdf:resource="#argues"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#judge -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;judge">
        <rdfs:range rdf:resource="&legal-case-ontology;Judge"/>
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;participant"/>
        <owl:propertyDisjointWith rdf:resource="&legal-case-ontology;party"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#judge_of -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;judge_of">
        <owl:inverseOf rdf:resource="&legal-case-ontology;judge"/>
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;participant_of"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#jurisdiction -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;jurisdiction">
        <rdfs:range rdf:resource="&legal-case-ontology;Jurisdiction"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#legal_value -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;legal_value"/>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#overturns -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;overturns">
        <rdfs:range rdf:resource="&legal-case-ontology;Decided_Case"/>
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;cites"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#participant -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;participant">
        <rdfs:domain rdf:resource="&legal-case-ontology;Case"/>
        <rdfs:range rdf:resource="&legal-case-ontology;Participant"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#participant_of -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;participant_of">
        <owl:inverseOf rdf:resource="&legal-case-ontology;participant"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#party -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;party">
        <rdfs:range rdf:resource="&legal-case-ontology;Party"/>
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;participant"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#party_of -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;party_of">
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;participant_of"/>
        <owl:inverseOf rdf:resource="&legal-case-ontology;party"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#party_representant_of -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;party_representant_of">
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;participant_of"/>
        <owl:inverseOf rdf:resource="&legal-case-ontology;party_representation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#party_representation -->

