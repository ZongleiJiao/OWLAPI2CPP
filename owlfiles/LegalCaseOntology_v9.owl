    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#Argument_Scheme -->

    <owl:Class rdf:about="&legal-case-ontology;Argument_Scheme">
        <rdfs:subClassOf>
            <owl:Restriction>
