        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&legal-case-ontology;decision"/>
            <rdf:Description rdf:about="&legal-case-ontology;rationale"/>
            <rdf:Description rdf:about="#disputed_evidence"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.wyner.info/research/case-ontology.owl#disputed_evidence -->

    <owl:ObjectProperty rdf:about="#disputed_evidence">
