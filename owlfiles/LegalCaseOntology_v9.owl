    </owl:ObjectProperty>
    


    <!-- http://www.wyner.info/research/case-ontology.owl#element -->

    <owl:ObjectProperty rdf:about="#element">
        <rdfs:domain rdf:resource="&legal-case-ontology;Case"/>
        <rdfs:range rdf:resource="#Element"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&legal-case-ontology;decision"/>
            <rdf:Description rdf:about="&legal-case-ontology;rationale"/>
            <rdf:Description rdf:about="#case_role"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.wyner.info/research/case-ontology.owl#evidence -->

    <owl:ObjectProperty rdf:about="#evidence">
