    <owl:ObjectProperty rdf:about="&legal-case-ontology;succeeds">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <owl:inverseOf rdf:resource="&legal-case-ontology;precedes"/>
    </owl:ObjectProperty>
    


