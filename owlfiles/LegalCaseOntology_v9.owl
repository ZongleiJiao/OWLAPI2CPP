    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#witness -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;witness">
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;participant"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&legal-case-ontology;decision"/>
            <rdf:Description rdf:about="&legal-case-ontology;rationale"/>
            <rdf:Description rdf:about="#testimony"/>
            <rdf:Description rdf:about="#given_by"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#witness_of -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;witness_of">
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;participant_of"/>
        <owl:inverseOf rdf:resource="&legal-case-ontology;witness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.wyner.info/research/case-ontology.owl#argues -->

    <owl:ObjectProperty rdf:about="#argues">
        <rdfs:subPropertyOf rdf:resource="#case_role"/>
    </owl:ObjectProperty>
    


    <!-- http://www.wyner.info/research/case-ontology.owl#case_role -->

    <owl:ObjectProperty rdf:about="#case_role"/>
    


    <!-- http://www.wyner.info/research/case-ontology.owl#cause_of_action -->

    <owl:ObjectProperty rdf:about="#cause_of_action">
