    <owl:ObjectProperty rdf:about="&legal-case-ontology;plaintiff">
        <rdfs:range rdf:resource="&legal-case-ontology;Plaintiff"/>
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;party"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#plaintiff_solicitor -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;plaintiff_solicitor">
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;party_representation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#precedes -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;precedes">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#rationale -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;rationale">
        <owl:inverseOf rdf:resource="#rationale_for"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#reporter -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;reporter">
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;participant"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#reporter_of -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;reporter_of">
        <rdfs:subPropertyOf rdf:resource="&legal-case-ontology;participant_of"/>
        <owl:inverseOf rdf:resource="&legal-case-ontology;reporter"/>
    </owl:ObjectProperty>
    


    <!-- http://www.best-project.nl/owl/legal-case-ontology.owl#solicitor -->

    <owl:ObjectProperty rdf:about="&legal-case-ontology;solicitor">
        <rdfs:domain rdf:resource="&legal-case-ontology;Party"/>
        <rdfs:range rdf:resource="&legal-case-ontology;Solicitor"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <owl:ObjectProperty>
                <owl:inverseOf rdf:resource="&legal-case-ontology;defendant"/>
            </owl:ObjectProperty>
            <rdf:Description rdf:about="&legal-case-ontology;defendant_solicitor"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <owl:ObjectProperty>
                <owl:inverseOf rdf:resource="&legal-case-ontology;plaintiff"/>
            </owl:ObjectProperty>
            <rdf:Description rdf:about="&legal-case-ontology;plaintiff_solicitor"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;Axiom"/>
        <rdfs:comment
            >Allows us to infer the solicitor relation between a defendant and the defendant_solicitor of a case</rdfs:comment>
        <owl:subject rdf:resource="&legal-case-ontology;solicitor"/>
        <owl:predicate rdf:resource="&owl;propertyChainAxiom"/>
        <owl:object rdf:parseType="Collection">
            <owl:ObjectProperty>
                <owl:inverseOf rdf:resource="&legal-case-ontology;defendant"/>
            </owl:ObjectProperty>
            <rdf:Description rdf:about="&legal-case-ontology;defendant_solicitor"/>
        </owl:object>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;Axiom"/>
        <rdfs:comment
            >allows us to infer the solicitor relation between a plaintiff and the plaintiff_solicitor of a case</rdfs:comment>
        <owl:subject rdf:resource="&legal-case-ontology;solicitor"/>
        <owl:predicate rdf:resource="&owl;propertyChainAxiom"/>
        <owl:object rdf:parseType="Collection">
            <owl:ObjectProperty>
                <owl:inverseOf rdf:resource="&legal-case-ontology;plaintiff"/>
            </owl:ObjectProperty>
            <rdf:Description rdf:about="&legal-case-ontology;plaintiff_solicitor"/>
        </owl:object>
    </rdf:Description>
    


