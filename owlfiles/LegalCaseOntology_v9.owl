    </owl:ObjectProperty>
    


    <!-- http://www.wyner.info/research/case-ontology.owl#decision_of -->

    <owl:ObjectProperty rdf:about="#decision_of"/>
    


    <!-- http://www.wyner.info/research/case-ontology.owl#disputed_element -->

    <owl:ObjectProperty rdf:about="#disputed_element">
